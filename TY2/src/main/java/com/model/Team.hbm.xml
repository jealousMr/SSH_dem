<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.model.Team" table="team" schema="imooc">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="teamName">
            <column name="teamName" sql-type="varchar(30)" length="30"/>
        </property>
        <property name="proName">
            <column name="proName" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <property name="captain">
            <column name="captain" sql-type="varchar(30)" length="30"/>
        </property>
        <property name="member1">
            <column name="member1" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <property name="member2">
            <column name="member2" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <property name="member3">
            <column name="member3" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <property name="member4">
            <column name="member4" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <property name="member5">
            <column name="member5" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <property name="message">
            <column name="message" sql-type="varchar(225)" length="225" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>