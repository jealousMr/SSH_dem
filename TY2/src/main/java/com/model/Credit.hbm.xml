<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.model.Credit" table="credit" schema="imooc">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="proName">
            <column name="proName" sql-type="varchar(30)" length="30"/>
        </property>
        <property name="publisher">
            <column name="publisher" sql-type="varchar(30)" length="30"/>
        </property>
        <property name="userNumber">
            <column name="userNumber" sql-type="varchar(30)" length="30"/>
        </property>
        <property name="count">
            <column name="count" sql-type="int(4)" not-null="true"/>
        </property>
        <property name="message">
            <column name="message" sql-type="varchar(225)" length="225" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>